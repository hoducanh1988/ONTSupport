<UserControl x:Class="VNPTRecalledProduct.SupportUserControl.ucHelp"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VNPTRecalledProduct.SupportUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Grid Margin="5">
        <Border BorderBrush="#777777" BorderThickness="1,1,1,1">
            <DocumentViewer x:Name="_docViewer" Background="White" Margin="5,5,5,5"/>
        </Border>
    </Grid>
    
</UserControl>
